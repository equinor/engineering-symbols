{
	"name": "@equinor/engineering-symbols",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/equinor/engineering-symbols"
	},
	"author": {
		"name": "Dzianis Makeichyk",
		"email": "dzm@equinor.com"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"license": "MIT",
	"scripts": {
		"test": "react-scripts test",
		"lint": "eslint --ext .ts --ext .tsx src",
		"ts:testcompile": "tsc --noEmit",
		"dev": "npm run storybook",
		"storybook": "start-storybook -p 3000",
		"build:storybook": "build-storybook",
		"build:lib": "rollup -c --environment NODE_ENV:production",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"prepublishOnly": "npm run build:lib",
		"checkcode": "npm run lint && npm run ts:testcompile && npm run prettier:check",
		"prepare": "husky install"
	},
	"engines": {
		"npm": ">= 8.3.1",
		"node": ">= 16.14.0"
	},
	"main": "build/index.cjs.js",
	"types": "build/types/index.d.ts",
	"module": "build/index.es.js",
	"files": [
		"/build"
	],
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			},
			{
				"files": [
					"**/*/*.*"
				],
				"rules": {
					"react/no-unknown-property": 2,
					"react-hooks/rules-of-hooks": "off",
					"react-hooks/exhaustive-deps": "off"
				}
			}
		]
	},
	"browser": {
		"fs": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"peerDependencies": {
		"react": "^16.14.0 || ^17.0.0",
		"react-dom": "^16.14.0 || ^17.0.0"
	},
	"dependencies": {
		"@emotion/react": "^11.9.0",
		"@rollup/plugin-json": "^4.1.0",
		"@types/jest": "^27.0.3",
		"@types/node": "^17.0.23",
		"@types/react": "^18.0.1",
		"@types/react-dom": "^18.0.0",
		"lint-staged": "^12.0.3",
		"rollup": "^2.60.2",
		"rollup-plugin-svg": "^2.0.0",
		"typescript": "^4.6.3"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-typescript": "^8.3.0",
		"@storybook/addon-actions": "^6.4.21",
		"@storybook/addon-essentials": "^6.4.9",
		"@storybook/addon-links": "^6.4.21",
		"@storybook/node-logger": "^6.4.21",
		"@storybook/preset-create-react-app": "^3.2.0",
		"@storybook/react": "^6.4.21",
		"@testing-library/dom": "^8.11.4",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"babel-loader": "^8.1.0",
		"husky": "^7.0.0",
		"prettier": "^2.6.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"storybook-addon-state": "^1.0.3",
		"storybook-addon-theme-playground": "^2.1.0"
	}
}
