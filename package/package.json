{
	"name": "@equinor/engineering-symbols",
	"version": "1.0.34",
	"repository": {
		"type": "git",
		"url": "https://github.com/equinor/engineering-symbols"
	},
	"author": {
		"name": "Dzianis Makeichyk",
		"email": "dzm@equinor.com"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"license": "MIT",
	"scripts": {
		"test": "react-scripts test",
		"start": "npm run storybook",
		"dev": "npm run storybook",
		"storybook": "start-storybook -p 3000",
		"build:svgs": "npx ts-node ./src/symbol-lib/build.ts",
		"build:storybook": "build-storybook",
		"build:lib": "rollup -c --environment NODE_ENV:production",
		"prepublishOnly": "npm run build:lib",
		"svgr": "npx @svgr/cli --typescript --no-dimensions --svgo-config ./svgo.config.js --out-dir src/components/icon/icons -- src/svg",
		"semantic-release": "semantic-release --branches master"
	},
	"engines": {
		"npm": ">= 8.3.1",
		"node": ">= 16.14.0"
	},
	"main": "build/index.cjs.js",
	"types": "build/types/index.d.ts",
	"module": "build/index.es.js",
	"files": [
		"/build"
	],
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			},
			{
				"files": [
					"**/*/*.*"
				],
				"rules": {
					"react/no-unknown-property": [
						"error",
						{
							"ignore": [
								"css"
							]
						}
					],
					"react-hooks/rules-of-hooks": "off",
					"testing-library/no-container": "off",
					"testing-library/no-node-access": "off",
					"react-hooks/exhaustive-deps": "off"
				}
			}
		]
	},
	"browser": {
		"fs": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"peerDependencies": {
		"react": "^16.14.0 || ^17.0.0 || ^18.0.0",
		"react-dom": "^16.14.0 || ^17.0.0 || ^18.0.0"
	},
	"dependencies": {
		"@rollup/plugin-json": "^4.1.0",
		"@types/react": "^18.0.6",
		"@types/react-dom": "^18.0.0",
		"rollup": "^2.60.2",
		"rollup-plugin-svg": "^2.0.0",
		"styled-components": "^5.3.5",
		"typescript": "^4.6.3"
	},
	"devDependencies": {
		"@cycjimmy/awesome-js-funcs": "^4.0.8",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-typescript": "^8.5.0",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/commit-analyzer": "^9.0.2",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^8.0.6",
		"@semantic-release/npm": "^9.0.1",
		"@storybook/addon-actions": "^6.4.21",
		"@storybook/addon-essentials": "^6.4.9",
		"@storybook/addon-links": "^6.4.21",
		"@storybook/node-logger": "^6.5.11",
		"@storybook/preset-create-react-app": "^3.2.0",
		"@storybook/react": "^6.4.21",
		"@svgr/cli": "^6.5.1",
		"@testing-library/dom": "^8.11.4",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"@types/node": "^18.7.23",
		"@types/styled-components": "^5.1.26",
		"babel-loader": "^8.2.5",
		"conventional-changelog-conventionalcommits": "^5.0.0",
		"eslint-config-react-app": "^7.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-import-css": "^3.0.3",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"semantic-release": "^19.0.5",
		"storybook-addon-theme-playground": "^2.1.0",
		"ts-node": "^10.9.1"
	}
}
